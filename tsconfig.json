{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "__tests__/Form.test.tsx", "__tests__/Form.test.tsx", "jest.setup.ts", "jest.config.ts"],
  "exclude": ["node_modules"],
  "types": [
      "node",
      "jest",
      "@testing-library/jest-dom"
    ],
}
